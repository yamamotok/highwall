{
  "name": "highwall",
  "version": "0.4.0",
  "description": "HighWall is a React component which fits viewport's height. Mobile browser compatible 100vh.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamamotok/highwall.git"
  },
  "files": [
    "dist/**/*"
  ],
