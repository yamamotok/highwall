{
  "name": "highwall",
  "version": "0.2.1",
  "description": "HighWall is a React component which fits viewport's height. It is like a mobile browser compatible 100vh div.",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamamotok/highwall.git"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/yamamotok/highwall",
  "main": "dist/HighWall.js",
  "types": "dist/HighWall.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist && tsc -p tsconfig.build.json && node postbuild.js",
    "test": "react-scripts test",
    "lint": "npm run prettier && npm run tslint",
    "prettier": "prettier --ignore-path .gitignore --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"./**/*.json\" \"./**/*.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "author": {
    "name": "Keisuke Yamamoto",
    "email": "keisuke@modelhouse.tech"
  },
  "keywords": [
    "typescript",
    "react",
    "mobile",
    "safari",
    "100vh"
  ],
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "rxjs": "^6.4.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/shelljs": "^0.8.7",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  }
}
