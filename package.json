  "main": "dist/src/HighWall.js",
  "types": "dist/HighWall.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "test": "jest",
    "prettier": "prettier --ignore-path .gitignore --write \"src/**/*.{ts,tsx,js,json,css,scss}\"",
    "eslint": "eslint --fix --ext .ts,.tsx src/",
    "format": "npm run prettier && npm run eslint"
  },
  "author": {
    "name": "Keisuke Yamamoto",
    "email": "keisuke@modelhouse.tech"
  },
  "keywords": [
    "typescript",
    "react",
    "mobile",
    "safari",
    "100vh"
  ],
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.5",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
